<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Portfolio</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
	<servlet-name>register</servlet-name>
	<servlet-class>admin.Registeration</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>register</servlet-name>
	<url-pattern>/admin/registeration.do</url-pattern>
  </servlet-mapping>

  <servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>admin.Login</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/admin/login.do</url-pattern>
  </servlet-mapping>

  <servlet>
	<servlet-name>Configuration</servlet-name>
	<servlet-class>admin.ConfigurationController</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>Configuration</servlet-name>
	<url-pattern>/admin/configuration</url-pattern>
  </servlet-mapping>

  <servlet>
	<servlet-name>Notice</servlet-name>
	<servlet-class>admin.NoticeController</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>Notice</servlet-name>
	<url-pattern>/admin/notice</url-pattern>
  </servlet-mapping>

  <servlet>
	<servlet-name>Notice2</servlet-name>
	<servlet-class>admin.NoticeDeleteController</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>Notice2</servlet-name>
	<url-pattern>/admin/notice-control</url-pattern>
  </servlet-mapping>
  
    <servlet>
	<servlet-name>updateAccess</servlet-name>
	<servlet-class>admin.ApprovalAccessController</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>updateAccess</servlet-name>
	<url-pattern>/admin/approval-access</url-pattern>
  </servlet-mapping>
</web-app>